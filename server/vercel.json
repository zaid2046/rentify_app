{
    "version": 2,
    "builds": [
        {
            "src": "/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js",
            "methods":["GET","POST", "PATCH", "PUT", "DELETE"],
            "headers":{
                "Access-Control-Allow-Origin": "https://rentify-eta-pink.vercel.app/"
            }
        }
    ]
}
